#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    int n;
  
    cin >> n;   //getting n
    int arr[n]; 
    for(int i=0;i<n;i++)  //getting the array
    {
        cin >> arr[i];
    }
    for(int j=n-1; j>=0; j--)
    {
      cout << arr[j] << " ";
    }
    return 0;
}
